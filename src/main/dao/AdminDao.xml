<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="findOrdersByPhone">
    <select id="findOrdersByPhone" parameterType="Integer" resultMap="Order">
        select * from tb_order where readerPhone = #{readerPhone}
    </select>
</mapper>